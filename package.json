{
  "name": "idle-task-que",
  "version": "0.1.0",
  "description": "A task manager that utilizes requestIdleCallback api to run tasks safely on the browser",
  "main": "./dist/index.js",
  "scripts": {
    "test": "ava",
    "tdd": "ava --watch",
    "build": "NODE_ENV=production webpack && NODE_ENV=development webpack",
    "lint": "./node_modules/.bin/eslint ./src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avivshaked/IdleTaskQue"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avivshaked/IdleTaskQue/issues"
  },
  "homepage": "https://github.com/avivshaked/IdleTaskQue#readme",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.3.0",
    "proxyquire": "^1.8.0",
    "sinon": "^2.3.5",
    "webpack": "^3.0.0"
  },
  "ava": {
    "files": [
      "./src/**/*.spec.js"
    ],
    "source": [
      "./src/**/*.js"
    ],
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  }
}
